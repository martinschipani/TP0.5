Object subclass: #Paciente	instanceVariableNames: 'nombre tarifa total'	classVariableNames: ''	package: 'tp0.5'!!Paciente methodsFor: 'initialization' stamp: 'MartinSchipani 10/5/2021 00:21'!initialize	total := 0.! !!Paciente methodsFor: 'accessing' stamp: 'MartinSchipani 10/4/2021 23:52'!nombre	^ nombre! !!Paciente methodsFor: 'accessing' stamp: 'MartinSchipani 10/5/2021 00:23'!totalPagado	^ total! !!Paciente methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/4/2021 23:52'!establecerTarifaObraSocial: tarifaObraSocial	tarifa := tarifaObraSocial.! !!Paciente methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/4/2021 23:59'!establecerTarifaParticular: tarifaParticular	tarifa := tarifaParticular.! !!Paciente methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/4/2021 23:31'!establecerNombre: unNombre	nombre := unNombre.! !!Paciente methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/5/2021 00:20'!recibirPago	total := total + tarifa.	^ tarifa! !Object subclass: #Testeo	instanceVariableNames: 'nombre tarifaObraSocial pacientes tarifaParticular total'	classVariableNames: ''	package: 'tp0.5'!!Testeo methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/5/2021 00:31'!obtenerNombresPacientes	^ (pacientes keys sorted) asOrderedCollection ! !!Testeo methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/5/2021 00:05'!obtenerCantidadDePacientes	^ pacientes size! !!Testeo methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/4/2021 23:28'!establecerNombre: nombreTesteo	 	nombre := nombreTesteo.! !!Testeo methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/5/2021 00:02'!obtenerTotal	^ total! !!Testeo methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/4/2021 23:59'!agregarPacienteParticular: paciente	paciente establecerTarifaParticular: tarifaParticular.	pacientes at: paciente nombre put: paciente.! !!Testeo methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/4/2021 23:29'!establecerTarifaObraSocial: tarifa	tarifaObraSocial := tarifa.! !!Testeo methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/5/2021 00:23'!cantidadPagadaPor: nombrePaciente	^ (pacientes at: nombrePaciente) totalPagado! !!Testeo methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/5/2021 00:03'!recibirPagoDe: nombrePaciente 	total := total + (pacientes at: nombrePaciente) recibirPago.! !!Testeo methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/4/2021 23:54'!establecerTarifaParticular: tarifa	tarifaParticular := tarifa.! !!Testeo methodsFor: 'as yet unclassified' stamp: 'MartinSchipani 10/4/2021 23:43'!agregarPacienteObraSocial: paciente 	paciente establecerTarifaObraSocial: tarifaObraSocial.	pacientes at: paciente nombre put: paciente.! !!Testeo methodsFor: 'initialization' stamp: 'MartinSchipani 10/5/2021 00:03'!initialize	pacientes := Dictionary new.	total := 0.! !